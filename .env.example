# Concepts Builder Environment Configuration
# Copy this file to .env and fill in your values

# =============================================================================
# API Keys (Required)
# =============================================================================

# Google Gemini API key for AI-powered content extraction
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase credentials for database uploads
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your_service_key_here

# =============================================================================
# Optional Configuration
# =============================================================================

# Gemini model to use (default: gemini-2.5-flash)
GEMINI_MODEL=gemini-2.5-flash

# Maximum concurrent uploads to Supabase (default: 1)
MAX_CONCURRENT_UPLOADS=1

# Maximum concurrent AI generations (default: 3)
MAX_CONCURRENT_GENERATIONS=3
